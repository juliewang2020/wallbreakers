class Solution(object):
    def numJewelsInStones(self, J, S):
        """
        :type J: str
        :type S: str
        :rtype: int
        """
    
        j = set(list(J))
        count = 0
        for i in range(0, len(S)): 
            if S[i] in J:
                count += 1
        return count
