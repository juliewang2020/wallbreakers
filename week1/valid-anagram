class Solution(object):
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        if len(t) == len(s):
            t = list(t)
            s = list(s)
            s.sort()
            t.sort()
            if  s == t:
                return True
            else:
                return False
        else:
            return False
