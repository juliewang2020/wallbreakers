class Solution(object):
    def fizzBuzz(self, n):
        """
        :type n: int
        :rtype: List[str]
        """
        numstr = [] 
        for num in range(1, n+1):   
            if num%3==0 and num%5==0:
                numstr.append("FizzBuzz")
            elif num%3 == 0:
                numstr.append("Fizz")
            elif num%5==0:
                numstr.append("Buzz") 
            else:
                numstr.append(str(num)) #cast num to string 
        return numstr
