class Solution(object):
    def isPowerOfTwo(self, n):
        """
        :type n: int
        :rtype: bool
        """
        n = n + 0.00
        while n  > 0:
            if n == 2 or n == 1:
                return True
            elif n > 2:
                n = n/2
                print(n)
            else:
                n=n/2
